    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "markdown-it": "^10.0.0",
    "mongoose": "^5.8.7",
    "next": "9.1.4",
    "raw-loader": "^4.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-markdown": "^4.2.2",
    "react-markdown-editor-lite": "^0.5.2"
  },
  "devDependencies": {
    "concurrently": "^5.0.2"
  }
